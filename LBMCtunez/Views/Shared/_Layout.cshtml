<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="jamz-nav">
        <div class="nav-container">
            <div class="nav-brand">
                @*@Html.ActionLink("LBMC Jamz", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
                <h2>LBMC Jamz</h2>
            </div>
            <div class="nav-links">
                <a href="#!/">Home</a>
                <a href="#!/jamz">Artists</a>
            </div>
        </div>
    </div>
    <div class="" ng-app="LBMCtunez">
        @RenderBody()
        @*<hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - LBMC Jamz</p>
            </footer>*@
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/angular")
    @RenderSection("scripts", required: false)
</body>
</html>
